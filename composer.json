{
    "name": "lizhineng/laravel-tablestore-cache-driver",
    "description": "Laravel cache driver for Alibaba Cloud Tablestore.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Li Zhineng",
            "email": "im@zhineng.li"
        }
    ],
    "require": {
        "php": "^8.0",
        "dew-serverless/tablestore-php": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-strict-rules": "^1.5",
        "laravel/pint": "^1.13",
        "rector/rector": "^0.18.12"
    },
    "autoload": {
        "psr-4": {
            "Zhineng\\Tablestore\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zhineng\\Tablestore\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zhineng\\Tablestore\\TablestoreServiceProvider"
            ]
        }
    },
    "scripts": {
        "format": "pint",
        "lint": "phpstan analyze",
        "refactor": "rector process",
        "test:style": "pint --test",
        "test:refactor": "rector process --dry-run",
        "test:integrate": "phpunit",
        "test": [
            "@test:style",
            "@test:refactor",
            "@lint",
            "@test:integrate"
        ]
    }
}
